{
  "name": "passhub",
  "version": "0.0.1",
  "description": "A password manager",
  "main": "index.js",
  "scripts": {
    "storybook": "start-storybook -p 2334 -c .storybook",
    "start-main": "webpack --config webpack/main.dev.config.js && electron dist/main.js",
    "start-renderer": "webpack-dev-server --config webpack/renderer.dev.config.js",
    "build-main": "webpack --config webpack/main.prod.config.js",
    "build-renderer": "webpack --config webpack/renderer.prod.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,webpack}/**/*.{js,ts,tsx,json,css,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "tslint --project ./tsconfig.json --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varHarrie/passhub.git"
  },
  "keywords": [],
  "author": "varHarrie<varharrie@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/varHarrie/passhub/issues"
  },
  "homepage": "https://github.com/varHarrie/passhub#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-knobs": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "@types/storybook__addon-knobs": "^4.0.2",
    "@types/storybook__react": "^4.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "electron": "^4.0.7",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.8.0",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.13.1",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.12",
    "@types/uuid": "^3.4.4",
    "persiston": "0.1.0-alpha.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-feather": "^1.1.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-react-hook": "^3.2.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2"
  }
}
