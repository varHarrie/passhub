{
  "name": "passhub",
  "version": "0.0.1",
  "description": "A password manager",
  "main": "index.js",
  "scripts": {
    "storybook": "start-storybook -p 2334 -c .storybook",
    "start-main": "webpack --config webpack/main.dev.config.js && electron dist/main.js",
    "start-renderer": "webpack-dev-server --config webpack/renderer.dev.config.js",
    "build-main": "webpack --config webpack/main.prod.config.js",
    "build-renderer": "webpack --config webpack/renderer.prod.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,webpack}/**/*.{js,ts,tsx,json,css,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "tslint --project ./tsconfig.json --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varHarrie/passhub.git"
  },
  "keywords": [],
  "author": "varHarrie<varharrie@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/varHarrie/passhub/issues"
  },
  "homepage": "https://github.com/varHarrie/passhub#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-knobs": "^5.1.3",
    "@storybook/react": "^5.1.3",
    "@types/storybook__addon-knobs": "^5.0.1",
    "@types/storybook__react": "^4.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "electron": "^5.0.3",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.0",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.9.0",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1",
    "url-loader": "^2.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@types/node": "^12.0.7",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.0",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^4.3.3",
    "@types/styled-components": "^4.1.16",
    "@types/uuid": "^3.4.4",
    "electron": "^5.0.3",
    "persiston": "^0.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^1.1.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.1",
    "uuid": "^3.3.2"
  }
}
