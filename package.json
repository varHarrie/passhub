{
  "name": "passhub",
  "version": "0.0.1",
  "description": "A password manager",
  "main": "index.js",
  "scripts": {
    "start-main": "webpack --config webpack/main.dev.config.js && electron dist/main.js",
    "start-renderer": "webpack-dev-server --config webpack/renderer.dev.config.js",
    "build-main": "webpack --config webpack/main.prod.config.js",
    "build-renderer": "webpack --config webpack/renderer.prod.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,webpack}/**/*.{js,ts,tsx,json,css,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "tslint --project ./tsconfig.json --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varHarrie/passhub.git"
  },
  "keywords": [],
  "author": "varHarrie<varharrie@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/varHarrie/passhub/issues"
  },
  "homepage": "https://github.com/varHarrie/passhub#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "babel-loader": "^8.0.5",
    "electron": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.3",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.6",
    "@types/uuid": "^3.4.4",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "persiston": "0.1.0-alpha.0",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-feather": "^1.1.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2"
  }
}
